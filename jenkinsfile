pipeline {
  agent any
  tools {nodejs "node"}
   
  stages {
    stage(cypress parallel tests) {

      parallel {
        stage('tester A') {
          steps{
            sh "npm run cy_run_browsers"
          }
        }
        stage('tester B') {
          agent {
            label 'mac'
          }
          steps {
            sh "npm run cy_run_browsers"
          }
        }
      }

    }
  }
}